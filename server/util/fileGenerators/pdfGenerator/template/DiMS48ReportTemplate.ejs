<!-- TODO dynamic padding and margin -->
<style type="text/css">
    body {
        font-family: sans-serif;
        font-weight: 200;
        margin: 50px;
        margin-top: 10px;
    }

    h1 {
        text-align: center;
    }

    h3 {
        text-decoration: underline;
    }

    table {
        width: 100%;
        border-collapse: collapse;
    }

    table tr td {
        border: 1px solid black;
        padding: 8px
    }

    .clientInfo h2 {
        text-align: center;
    }

    .main-page-header {
        padding-top: 1vh;
        width: 100%;
        margin-left: 0;
        position: relative;
    }

    .main-page-header img {
        height: 5vh;
        width: auto;
        margin: auto;
        float: left;
        margin-top: 20px;
    }

    .main-page-header h1 {
        align-self: baseline;
    }

    .scores {
        padding-bottom: 130px;
    }

    h2 {
        text-decoration: underline;
    }

    table {
        border: 1px solid black;
    }

    .scores table tr:last-child { 
        border-top: solid black 3px;
        text-decoration: underline;
        background-color: rgba(230, 225, 225, 0.699);
     }

    .correct {
        background-color: rgba(0, 128, 0, 0.65);
    }

    .wrong {
        background-color: rgba(255, 0, 0, 0.64);
    }

    .noAnswer {
        background-color: gray !important;
    }

    .capitalize-all-first-letter {
        text-transform: capitalize;
    }

    .capitalize-first-word:first-letter {
        text-transform: uppercase;
    }

    .page {
        height: 100vh;
    }

    #notes-container {
        height: 80px;
    }

    .clientInfo table tr td:first-child,
    .scores table tr td:first-child {
        width: 25vw;
    }

    #test-id {
        text-decoration: underline;
        font-weight: bold;
        font-size: 1.2em;
    }
</style>

